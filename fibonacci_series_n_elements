#include<stdio.h>
int main()
{
    //program for  fibonacci series e.g. 0,1,1,2,3,5,8
    //program to  print n elements of a  fibonacci  series
    int n,i; 
    printf("\nEnter the number =  ");
    scanf("%d",&n);
    int a[n];
    a[0]=0;
    a[1]=1;
    for(i=2;i<n;i++)
    {
        a[i]=a[i-1]+a[i-2];
        
    }
    for(i=0;i<n;i++)
    {
        printf("%d\t",a[i]);
    }
    return 0;
}
